
@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@model Assessment01.Models.MoviedbEntities

<h2>Edit</h2>

<form asp-action="Edit">
    <input type="hidden" asp-for="Mid" />
    <div class="form-group">
        <label asp-for="MovieName" class="control-label"></label>
        <input asp-for="MovieName" class="form-control" />
        <span asp-validation-for="MovieName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="DateOfRelease" class="control-label"></label>
        <input asp-for="DateOfRelease" class="form-control" />
        <span asp-validation-for="DateOfRelease" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
</form>




